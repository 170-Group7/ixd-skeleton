						<option value="1">CSE 170- Lecture 1</option>
						<option value="2">CSE 170- Lecture 2</option>
						<option value="3">CSE 170- Lecture 3</option>
					</select>
				</div>
			</div>

			<div class="row mb-4">
				<label for="date" class="col-sm-2 col-form-label">Date</label>
				<div class="col-sm-10">
   					 <input class="form-control" type="date" value="2021-03-03" id="date-input">
  				</div>
			</div>

			<div class="row mb-4">
  				<label for="time-input" class="col-sm-2 col-form-label">Time</label>
  				<div class="col-sm-10">
    				<input class="form-control" type="time" value="13:45:00" id="time-input">
  				</div>
			</div>

			<div class="row mb-4">
				<div class="col-sm-2"></div>
				<div class="col-sm">
					{{!-- <a class="btn btn-warning " href="/confirmation" role="button">Invite</a> --}}
