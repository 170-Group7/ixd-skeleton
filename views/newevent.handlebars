<body>
	<div class="container">
		<div class="row title">
			<div class="col-sm-2 d-flex align-items-center justify-content-start">
				<a class="btn btn-warning " href="/index" role="button">Back</a>
    		</div>
			<div class="col-sm d-flex justify-content-center">
				<h1>New Study Group Schedule</h1>
			</div>
			<div class="col-sm-2">
				
			</div>
			
		</div>
		<div class="row mb-4 mt-4">
			<div class="col-sm"></div>
			<div class="col-sm-8">
				<div class="progress">
					<div class="progress-bar" role="progressbar" style="width: 33%;" aria-valuenow="33" aria-valuemin="0" aria-valuemax="100">Create</div>
				</div>
			</div>
			<div class="col-sm"></div>
        </div>
		<div class="form-group">
			<div class="row mb-4">
				<label for="lecture" class="col-sm-2 col-form-label">Lecture</label>
				<div class="col-sm-10">
					<select class="form-select" aria-label="Default select example" id="lecture-Select">
